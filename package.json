{
  "name": "animetarr",
  "version": "1.1.0",
  "description": "Pronounced 'Animator', this service provides an interface to review and add anime to Sonarr and Radarr based on release schedule.",
  "main": "dist/app.js",
  "scripts": {
    "build": "./node_modules/.bin/ncc build -m --license LICENSE.md src/app.ts",
    "start": "./node_modules/.bin/tsc && node dist/app.js",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags && rm -rf dist"
  },
  "author": "Hunter Horsman",
  "license": "ISC",
  "dependencies": {
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "node-tvdb": "^4.1.0"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.12",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.29.0",
    "typescript": "^4.3.4"
  }
}
